Route {
	UrlPattern urlPattern,
	Regex[] qsPattern,
	String? httpMethod,
	Array routeData,
	
	array|null match()
}


IRouter {
	RouteData route(WebRequest request) : throws RouteException
}


Router {
	Array routes,
	RouteData defaultRouteData,
	
	RouteData route(WebRequest request) : throws RouteException
}




IDispatcher {
	void dispatch(RouteData routeData, IWebContext webContext)
}




IControllerContext {
	IWebContext webContext,
	RouteData routeData
}


IController {
	void handle(IControllerContext controllerContext)
}





IView {
	string render()
}

View {
	_construct(ViewData viewData)
}





IActionResult {
	void handle(IWebContext webContext)
}

ViewActionResult {
	_construct(View view)
}
